{
  "name": "code-search",
  "version": "0.1.0",
  "description": "Ultra-simple code search tool with Jina embeddings, LanceDB, and MCP protocol support",
  "type": "module",
  "bin": {
    "code-search": "./bin/code-search.js",
    "code-search-mcp": "./mcp.js"
  },
  "main": "src/cli.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node ./bin/code-search.js",
    "postinstall": "node scripts/patch-transformers.js"
  },
  "dependencies": {
    "@lancedb/vectordb-win32-x64-msvc": "^0.21.2",
    "@modelcontextprotocol/sdk": "1.0.0",
    "@xenova/transformers": "^2.17.0",
    "apache-arrow": "^13.0.0",
    "fast-glob": "^3.3.2",
    "onnxruntime-node": "^1.23.2",
    "onnxruntime-web": "^1.19.0",
    "vectordb": "^0.21.2"
  },
  "optionalDependencies": {
    "sharp": "^0.34.5"
  },
  "keywords": [
    "code-search",
    "embeddings",
    "vector-db",
    "jina",
    "lancedb",
    "mcp"
  ]
}
